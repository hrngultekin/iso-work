all:
	pyuic5 -o gui/ui/main.py gui/ui/main.ui
	pyuic5 -o gui/ui/mainv2.py gui/ui/mainv2.ui
	pyuic5 -o gui/ui/languages.py gui/ui/rawlanguages.ui
	pyuic5 -o gui/ui/packages.py gui/ui/packages.ui
	pyuic5 -o gui/ui/packagecollection.py gui/ui/packagecollection.ui
	pyrcc5 -o raw_rc.py gui/ui/raw.qrc
	# pyrcc5 -o gui/ui/raw_rc.py gui/ui/raw.qrc

clean:
	find .  -name *.pyc | xargs rm -rf
	rm -rf gui/ui/main.py
	rm -rf gui/ui/mainv2.py
	rm -rf gui/ui/languages.py
	rm -rf gui/ui/packages.py
	rm -rf gui/ui/packagecollection.py
	rm -rf gui/ui/raw_rc.py
